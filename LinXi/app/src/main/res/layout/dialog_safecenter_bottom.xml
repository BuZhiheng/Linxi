<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    >
    <Button
        android:id="@+id/btn_dialog_safecenter_sure"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_title_height"
        android:layout_marginLeft="@dimen/common_margin"
        android:layout_marginRight="@dimen/common_margin"
        android:background="@drawable/bg_ll_station_goods_yellow"
        android:text="@string/app_name"
        android:textSize="@dimen/text_common_item_title"
        android:textColor="@color/color_white"
        />
    <Button
        android:id="@+id/btn_dialog_safecenter_cancel"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_title_height"
        android:layout_margin="@dimen/common_margin"
        android:background="@drawable/bg_ll_mainfrm_nostroke"
        android:text="取消"
        android:textSize="@dimen/text_common_item_title"
        android:textColor="@color/color_black_text"
        />
</LinearLayout>